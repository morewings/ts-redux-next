  "dependencies": {
    "axios": "1.6.1",
    "next": "14.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "redux": "4.2.1",
    "uniqid": "5.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.6",
    "@types/node": "18.18.9",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/uniqid": "5.3.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "eslint": "8.53.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "4.2.1",
